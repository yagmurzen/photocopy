            $(".input-slug").change(function () {


                var ss = $(".input-slug").val().replace(/Ğ/gim, "g")
                    .replace(/Ü/gim, "u")
                    .replace(/Ş/gim, "s")
                    .replace(/I/gim, "i")
                    .replace(/İ/gim, "i")
                    .replace(/Ö/gim, "o")
                    .replace(/Ç/gim, "c")
                    .replace(/ğ/gim, "g")
                    .replace(/ü/gim, "u")
                    .replace(/ş/gim, "s")
                    .replace(/ı/gim, "i")
                    .replace(/ö/gim, "o")
                    .replace(/ç/gim, "c");

                $(".input-slug-text").val(ss.toLowerCase()
                    .replace(/[^\w ]+/g, '')
                    .replace(/ +/g, '-'));
            });

              //$('.nav .nav-item').click(function () {

              //  $(this).addClass("active")
              //});
        });
    </script>

